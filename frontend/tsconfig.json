{
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(main)/(tabs)/_layout.js",
    "app/_layout.jsx",
    "app/(auth)/welcome.jsx",
    "components/ItemContainer.jsx"
  ],
  "extends": "expo/tsconfig.base"
}
